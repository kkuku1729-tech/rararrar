local function applySpeed()
    local newSpeed = tonumber(SpeedInputBox.Text)
    if newSpeed and newSpeed > 0 then
        FlySpeed = newSpeed
        SpeedInputBox.Text = tostring(FlySpeed)
        
        -- –ê–Ω–∏–º–∞—Ü–∏—è —É—Å–ø–µ—à–Ω–æ–≥–æ –ø—Ä–∏–º–µ–Ω–µ–Ω–∏—è
        ApplySpeedButton.Text = "‚úÖ –°–ö–û–†–û–°–¢–¨ –£–°–¢–ê–ù–û–í–õ–ï–ù–ê!"
        wait(1)
        ApplySpeedButton.Text = "‚úÖ –ü–†–ò–ú–ï–ù–ò–¢–¨ –°–ö–û–†–û–°–¢–¨"
    else
        -- –ê–Ω–∏–º–∞—Ü–∏—è –æ—à–∏–±–∫–∏
        ApplySpeedButton.Text = "‚ùå –ù–ï–í–ï–†–ù–û–ï –ó–ù–ê–ß–ï–ù–ò–ï!"
        ApplySpeedButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
        wait(1)
        ApplySpeedButton.Text = "‚úÖ –ü–†–ò–ú–ï–ù–ò–¢–¨ –°–ö–û–†–û–°–¢–¨"
        ApplySpeedButton.BackgroundColor3 = COLOR_ACCENT
        SpeedInputBox.Text = tostring(FlySpeed)
    end
end

-- –§—É–Ω–∫—Ü–∏—è Fly Hack
local function toggleFly()
    Flying = not Flying
    
    if Flying then
        FlyButton.Text = "üöÄ FLY HACK: ON"
        FlyButton.BackgroundColor3 = Color3.fromRGB(0, 200, 83)
        
        local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
        
        BodyVelocity = Instance.new("BodyVelocity")
        BodyGyro = Instance.new("BodyGyro")
        
        BodyVelocity.Velocity = Vector3.new(0, 0, 0)
        BodyVelocity.MaxForce = Vector3.new(400000, 400000, 400000)
        BodyVelocity.Parent = HumanoidRootPart
        
        BodyGyro.MaxTorque = Vector3.new(400000, 400000, 400000)
        BodyGyro.P = 100000
        BodyGyro.D = 1000
        BodyGyro.Parent = HumanoidRootPart
        
        -- –ê–Ω–∏–º–∞—Ü–∏—è –ø–æ–ª–µ—Ç–∞
        local flyConnection
        flyConnection = RunService.Heartbeat:Connect(function()
            if not Flying then
                flyConnection:Disconnect()
                return
            end
            
            local camera = workspace.CurrentCamera
            BodyGyro.CFrame = camera.CFrame
            
            local direction = Vector3.new()
            
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                direction = direction + camera.CFrame.LookVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                direction = direction - camera.CFrame.LookVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                direction = direction - camera.CFrame.RightVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                direction = direction + camera.CFrame.RightVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                direction = direction + Vector3.new(0, 1, 0)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                direction = direction - Vector3.new(0, 1, 0)
            end
            
            BodyVelocity.Velocity = direction * FlySpeed
        end)
        
    else
        FlyButton.Text = "üöÄ FLY HACK: OFF"
        FlyButton.BackgroundColor3 = COLOR_PRIMARY
        
        if BodyVelocity then
            BodyVelocity:Destroy()
            BodyVelocity = nil
        end
        if BodyGyro then
            BodyGyro:Destroy()
            BodyGyro = nil
        end
    end
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ —Å–æ–±—ã—Ç–∏–π
